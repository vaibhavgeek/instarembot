<% if @login_allow == "true"  %>
	$("#show").append("<%= j render( 'bot/partials/prev') %>")
	$("#message_alert").html("<center> It seems you have already logged in. Here is what you can do.</center> ");
<% else %>
	$("#show").append("<%= j render('/bot/partials/initial') %>");
<% end %>
	$("html, body").animate({ scrollTop: $(document).height() }, 1000);
$("#message_input").autocomplete({ disabled: true });
